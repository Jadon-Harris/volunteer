.selectView {
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 60rpx;
  border-bottom: 3rpx solid #7A7E83;
}

.timeView {
  margin-left: 48rpx;
  position: relative;
  display: flex;
  flex-direction: row;
}

.positionView {
  margin-left: 120rpx;
  position: relative;
  display: flex;
  flex-direction: row;
}

.text {
  align-self: center;
  color: #7A7E83;
}

.imageView {
  margin: 12rpx 0rpx 12rpx 12rpx;
  display: flex;
  flex-direction: column;
  align-self: center;
}

.image {
  height: 18rpx;
  width: 18rpx;
}