<!--pages/ex_sucess/ex_sucess.wxml-->
<view class="page">
    <view class="weui-msg">
        <view class="weui-msg__icon-area">
            <icon type="success" size="64"></icon>
        </view>
        <view class="weui-msg__text-area">
            <view class="weui-msg__title">操作成功</view>
            <view class="weui-msg__desc">您已兑换成功<navigator url="" class="weui-msg__link"></navigator></view>
        </view>
        <view class="weui-msg__opr-area">
            <view class="weui-btn-area">
                <button class="weui-btn" type="primary" bindtap="queren">确认</button>
            </view>
        </view>
        <view class="weui-msg__tips-area">
            <view class="weui-msg__tips">点击确认回到兑换界面继续兑换<navigator url="" class="weui-msg__link"></navigator></view>
        </view>
        <view class="weui-msg__extra-area">
            <view class="weui-footer">
                <view class="weui-footer__links">
                    <navigator url="" class="weui-footer__link"></navigator>
                </view>
                <view class="weui-footer__text">Code by Zhaozhao 5-11</view>
            </view>
        </view>
    </view>
</view>

